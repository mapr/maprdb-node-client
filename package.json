{
  "name": "node-maprdb",
  "version": "0.0.4",
  "description": "",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "npm run proto:dev && mocha -r ts-node/register --compiler tsc --project ./tsconfig.json --watch-extensions ts test/**/*.ts -w",
    "clean": "rimraf ./dist/* ./test/dist/*",
    "prepublish": "npm run build",
    "proto": "mkdirp ./dist/proto && pbjs -t static-module -w commonjs ./proto/maprdb-server.proto -o ./dist/proto/index.js && pbts -o ./dist/proto/index.d.ts ./dist/proto/index.js",
    "proto:dev": "pbjs -t static-module -w commonjs ./proto/maprdb-server.proto -o ./src/proto/index.js && pbts -o ./src/proto/index.d.ts ./src/proto/index.js",
    "build": "npm run proto && tsc -p tsconfig.release.json",
    "watch": "tsc -w",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/promise-retry": "^1.1.1",
    "@types/retry": "^0.10.2",
    "bluebird": "^3.5.1",
    "dotenv": "^5.0.0",
    "grpc": "^1.11.3",
    "grpc-health-check": "^1.6.6",
    "lodash": "^4.17.5",
    "promise-retry": "^1.1.1",
    "retry": "^0.12.0"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.20",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.48",
    "@types/node": "^9.4.0",
    "chai": "^4.1.2",
    "jest": "^22.1.4",
    "mkdirp": "^0.5.1",
    "mocha": "^5.0.1",
    "protobufjs": "^6.8.5",
    "rimraf": "^2.6.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.2",
    "tsutils": "^2.21.0",
    "typescript": "^2.7.1"
  }
}
